{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "indent": [4, "tab"],
        "no-tabs": 0,
        "no-underscore-dangle": 0,
        "comma-dangle": 0,
        "max-len": 0,
        "function-paren-newline": 0,
        "implicit-arrow-linebreak": 0
    }
}
